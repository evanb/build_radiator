<h1>Listing build_plans</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
  </tr>

<% for build_plan in @build_plans %>
  <tr>
    <td><%=h build_plan.name %></td>
    <td><%=h build_plan.url %></td>
    <td><%= link_to 'Show', build_plan %></td>
    <td><%= link_to 'Edit', edit_build_plan_path(build_plan) %></td>
    <td><%= link_to 'Destroy', build_plan, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New build_plan', new_build_plan_path %>
